{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "pocadf2025"
		},
		"azure_ddl_storage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'azure_ddl_storage'"
		},
		"azure_sql_connection_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'azure_sql_connection'"
		},
		"azure_ddl_storage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://pocdata2025.dfs.core.windows.net/"
		},
		"azure_sql_connection_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "spotifyazureprojectserver.database.windows.net"
		},
		"azure_sql_connection_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "spotify_db"
		},
		"azure_sql_connection_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "skbasha"
		},
		"tr-storage-event_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/6bbdd632-f1d6-4c0c-8b40-49cb5f250742/resourceGroups/poc_adf_learn/providers/Microsoft.Storage/storageAccounts/pocdata2025"
		},
		"tr-storage_event_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/6bbdd632-f1d6-4c0c-8b40-49cb5f250742/resourceGroups/poc_adf_learn/providers/Microsoft.Storage/storageAccounts/pocdata2025"
		},
		"TR_Storage_Event_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/6bbdd632-f1d6-4c0c-8b40-49cb5f250742/resourceGroups/poc_adf_learn/providers/Microsoft.Storage/storageAccounts/pocdata2025"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "pocadf2025_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"azure_ddl_storage_accountKey": {
						"value": "[parameters('azure_ddl_storage_accountKey')]"
					},
					"azure_sql_connection_password": {
						"value": "[parameters('azure_sql_connection_password')]"
					},
					"azure_ddl_storage_properties_typeProperties_url": {
						"value": "[parameters('azure_ddl_storage_properties_typeProperties_url')]"
					},
					"azure_sql_connection_properties_typeProperties_server": {
						"value": "[parameters('azure_sql_connection_properties_typeProperties_server')]"
					},
					"azure_sql_connection_properties_typeProperties_database": {
						"value": "[parameters('azure_sql_connection_properties_typeProperties_database')]"
					},
					"azure_sql_connection_properties_typeProperties_userName": {
						"value": "[parameters('azure_sql_connection_properties_typeProperties_userName')]"
					},
					"tr-storage-event_properties_typeProperties_scope": {
						"value": "[parameters('tr-storage-event_properties_typeProperties_scope')]"
					},
					"tr-storage_event_properties_typeProperties_scope": {
						"value": "[parameters('tr-storage_event_properties_typeProperties_scope')]"
					},
					"TR_Storage_Event_properties_typeProperties_scope": {
						"value": "[parameters('TR_Storage_Event_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}
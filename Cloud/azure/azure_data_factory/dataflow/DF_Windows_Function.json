{
	"name": "DF_Windows_Function",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText23",
						"type": "DatasetReference"
					},
					"name": "EmpData"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "window1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          emp_id as integer,",
				"          name as string,",
				"          salary as integer,",
				"          loc as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> EmpData",
				"EmpData window(over(loc),",
				"     desc(salary, true),",
				"     cummulative_salary = sum(salary),",
				"          emp_rank = denseRank()) ~> window1"
			]
		}
	}
}